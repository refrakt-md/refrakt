{
  "name": "@refrakt-md/lumina",
  "description": "Lumina theme for refrakt.md â€” design tokens, CSS, identity transform, and framework adapters",
  "version": "0.4.0",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/refrakt-md/refrakt.git",
    "directory": "packages/lumina"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/transform.js",
  "types": "dist/transform.d.ts",
  "exports": {
    ".": "./index.css",
    "./base.css": "./base.css",
    "./transform": {
      "types": "./dist/transform.d.ts",
      "default": "./dist/transform.js"
    },
    "./manifest": "./manifest.json",
    "./contracts": "./contracts/structures.json",
    "./svelte": {
      "types": "./svelte/index.ts",
      "svelte": "./svelte/index.ts",
      "default": "./svelte/index.ts"
    },
    "./styles/runes/*.css": "./styles/runes/*.css",
    "./svelte/tokens.css": "./svelte/tokens.css",
    "./svelte/manifest": "./svelte/manifest.json"
  },
  "files": [
    "dist",
    "base.css",
    "index.css",
    "tokens",
    "styles",
    "contracts",
    "manifest.json",
    "svelte"
  ],
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@refrakt-md/theme-base": "0.4.0",
    "@refrakt-md/transform": "0.4.0",
    "@refrakt-md/types": "0.4.0",
    "@refrakt-md/svelte": "0.4.0"
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "@sveltejs/kit": "^2.0.0"
  },
  "devDependencies": {
    "postcss": "^8.4.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    },
    "@sveltejs/kit": {
      "optional": true
    }
  }
}
