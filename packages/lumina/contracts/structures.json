{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "HTML structure contracts for the Lumina theme. Documents the BEM selectors, data attributes, and HTML structure the identity transform produces for each rune. Generated from baseConfig in packages/theme-base/src/config.ts and the engine in packages/transform/src/engine.ts.",
  "prefix": "rf",
  "runes": {

    "Accordion": {
      "block": "accordion",
      "root": ".rf-accordion",
      "dataRune": "accordion",
      "childOrder": ["{content}"]
    },

    "AccordionItem": {
      "block": "accordion-item",
      "root": ".rf-accordion-item",
      "dataRune": "accordionitem",
      "elements": {
        "header": { "tag": "name", "selector": ".rf-accordion-item__header", "source": "autoLabel" }
      },
      "childOrder": ["{content}"],
      "notes": "autoLabel maps <name> child tags to data-name='header'"
    },

    "Details": {
      "block": "details",
      "root": ".rf-details",
      "dataRune": "details",
      "elements": {
        "summary": { "tag": "summary", "selector": ".rf-details__summary", "source": "autoLabel" }
      },
      "childOrder": ["{content}"],
      "notes": "autoLabel maps <summary> child tags to data-name='summary'"
    },

    "Grid": {
      "block": "grid",
      "root": ".rf-grid",
      "dataRune": "grid",
      "childOrder": ["{content}"]
    },

    "CodeGroup": {
      "block": "codegroup",
      "root": ".rf-codegroup",
      "dataRune": "codegroup",
      "modifiers": {
        "title": {
          "source": "meta",
          "classPattern": ".rf-codegroup--{value}",
          "dataAttribute": "data-title"
        }
      },
      "elements": {
        "topbar": { "tag": "div", "selector": ".rf-codegroup__topbar", "source": "structure" },
        "dot": { "tag": "span", "selector": ".rf-codegroup__dot", "source": "structure", "parent": "topbar" },
        "title": { "tag": "span", "selector": ".rf-codegroup__title", "source": "structure", "parent": "topbar", "condition": "title" }
      },
      "childOrder": ["topbar", "{content}"]
    },

    "PageSection": {
      "block": "page-section",
      "root": ".rf-page-section",
      "dataRune": "pagesection",
      "childOrder": ["{content}"]
    },

    "TableOfContents": {
      "block": "toc",
      "root": ".rf-toc",
      "dataRune": "tableofcontents",
      "childOrder": ["{content}"]
    },

    "Embed": {
      "block": "embed",
      "root": ".rf-embed",
      "dataRune": "embed",
      "childOrder": ["{content}"],
      "notes": "Interactive rune — Svelte component (Embed.svelte) handles rendering"
    },

    "Breadcrumb": {
      "block": "breadcrumb",
      "root": ".rf-breadcrumb",
      "dataRune": "breadcrumb",
      "childOrder": ["{content}"]
    },

    "BreadcrumbItem": {
      "block": "breadcrumb-item",
      "root": ".rf-breadcrumb-item",
      "dataRune": "breadcrumbitem",
      "childOrder": ["{content}"]
    },

    "Testimonial": {
      "block": "testimonial",
      "root": ".rf-testimonial",
      "dataRune": "testimonial",
      "childOrder": ["{content}"],
      "notes": "Interactive rune — Svelte component (Testimonial.svelte) handles rendering"
    },

    "Timeline": {
      "block": "timeline",
      "root": ".rf-timeline",
      "dataRune": "timeline",
      "modifiers": {
        "direction": {
          "source": "meta",
          "default": "vertical",
          "classPattern": ".rf-timeline--{value}",
          "dataAttribute": "data-direction"
        }
      },
      "childOrder": ["{content}"]
    },

    "TimelineEntry": {
      "block": "timeline-entry",
      "root": ".rf-timeline-entry",
      "dataRune": "timelineentry",
      "childOrder": ["{content}"]
    },

    "Changelog": {
      "block": "changelog",
      "root": ".rf-changelog",
      "dataRune": "changelog",
      "childOrder": ["{content}"]
    },

    "ChangelogRelease": {
      "block": "changelog-release",
      "root": ".rf-changelog-release",
      "dataRune": "changelogrelease",
      "childOrder": ["{content}"]
    },

    "Event": {
      "block": "event",
      "root": ".rf-event",
      "dataRune": "event",
      "modifiers": {
        "date": {
          "source": "meta",
          "classPattern": ".rf-event--{value}",
          "dataAttribute": "data-date"
        },
        "endDate": {
          "source": "meta",
          "classPattern": ".rf-event--{value}",
          "dataAttribute": "data-end-date"
        },
        "location": {
          "source": "meta",
          "classPattern": ".rf-event--{value}",
          "dataAttribute": "data-location"
        },
        "url": {
          "source": "meta",
          "classPattern": ".rf-event--{value}",
          "dataAttribute": "data-url"
        }
      },
      "elements": {
        "details": { "tag": "div", "selector": ".rf-event__details", "source": "structure" },
        "detail": { "tag": "div", "selector": ".rf-event__detail", "source": "structure", "parent": "details" },
        "label": { "tag": "span", "selector": ".rf-event__label", "source": "structure", "parent": "detail" },
        "value": { "tag": "span", "selector": ".rf-event__value", "source": "structure", "parent": "detail" },
        "end-date": { "tag": "span", "selector": ".rf-event__end-date", "source": "structure", "parent": "detail", "condition": "endDate" },
        "register": { "tag": "a", "selector": ".rf-event__register", "source": "structure", "parent": "details", "condition": "url" },
        "content": { "tag": "div", "selector": ".rf-event__content", "source": "contentWrapper" }
      },
      "childOrder": ["details", "{content:content}"],
      "notes": "Event modifiers are freeform values (dates, locations, URLs) — use [data-*] attribute selectors, not BEM modifier classes"
    },

    "Organization": {
      "block": "organization",
      "root": ".rf-organization",
      "dataRune": "organization",
      "childOrder": ["{content}"]
    },

    "Cast": {
      "block": "cast",
      "root": ".rf-cast",
      "dataRune": "cast",
      "childOrder": ["{content}"]
    },

    "CastMember": {
      "block": "cast-member",
      "root": ".rf-cast-member",
      "dataRune": "castmember",
      "childOrder": ["{content}"]
    },

    "Recipe": {
      "block": "recipe",
      "root": ".rf-recipe",
      "dataRune": "recipe",
      "modifiers": {
        "prepTime": {
          "source": "meta",
          "classPattern": ".rf-recipe--{value}",
          "dataAttribute": "data-prep-time"
        },
        "cookTime": {
          "source": "meta",
          "classPattern": ".rf-recipe--{value}",
          "dataAttribute": "data-cook-time"
        },
        "servings": {
          "source": "meta",
          "classPattern": ".rf-recipe--{value}",
          "dataAttribute": "data-servings"
        },
        "difficulty": {
          "source": "meta",
          "default": "medium",
          "classPattern": ".rf-recipe--{value}",
          "dataAttribute": "data-difficulty"
        }
      },
      "elements": {
        "meta": { "tag": "div", "selector": ".rf-recipe__meta", "source": "structure", "conditionAny": ["prepTime", "cookTime", "servings", "difficulty"] },
        "meta-item": { "tag": "span", "selector": ".rf-recipe__meta-item", "source": "structure", "parent": "meta" },
        "badge": { "tag": "span", "selector": ".rf-recipe__badge", "source": "structure", "parent": "meta", "condition": "difficulty" },
        "content": { "tag": "div", "selector": ".rf-recipe__content", "source": "contentWrapper" }
      },
      "childOrder": ["meta", "{content:content}"],
      "notes": "meta-item elements show prepTime (duration-transformed), cookTime (duration-transformed), and servings. Badge shows difficulty."
    },

    "RecipeIngredient": {
      "block": "recipe-ingredient",
      "root": ".rf-recipe-ingredient",
      "dataRune": "recipeingredient",
      "childOrder": ["{content}"],
      "notes": "Styled inline within recipe.css, no own block-level CSS file"
    },

    "Pricing": {
      "block": "pricing",
      "root": ".rf-pricing",
      "dataRune": "pricing",
      "childOrder": ["{content}"]
    },

    "Tier": {
      "block": "tier",
      "root": ".rf-tier",
      "dataRune": "tier",
      "childOrder": ["{content}"]
    },

    "FeaturedTier": {
      "block": "tier",
      "root": ".rf-tier",
      "dataRune": "featuredtier",
      "staticModifiers": [
        { "name": "featured", "selector": ".rf-tier--featured" }
      ],
      "childOrder": ["{content}"],
      "notes": "Shares block name 'tier' with Tier. The --featured static modifier is always applied."
    },

    "Feature": {
      "block": "feature",
      "root": ".rf-feature",
      "dataRune": "feature",
      "modifiers": {
        "split": {
          "source": "meta",
          "classPattern": ".rf-feature--{value}",
          "dataAttribute": "data-split"
        },
        "mirror": {
          "source": "meta",
          "classPattern": ".rf-feature--{value}",
          "dataAttribute": "data-mirror"
        }
      },
      "contextModifiers": {
        "Hero": { "suffix": "in-hero", "selector": ".rf-feature--in-hero" },
        "Grid": { "suffix": "in-grid", "selector": ".rf-feature--in-grid" }
      },
      "childOrder": ["{content}"]
    },

    "FeatureDefinition": {
      "block": "feature-definition",
      "root": ".rf-feature-definition",
      "dataRune": "featuredefinition",
      "childOrder": ["{content}"]
    },

    "Steps": {
      "block": "steps",
      "root": ".rf-steps",
      "dataRune": "steps",
      "childOrder": ["{content}"]
    },

    "Step": {
      "block": "step",
      "root": ".rf-step",
      "dataRune": "step",
      "modifiers": {
        "split": {
          "source": "meta",
          "classPattern": ".rf-step--{value}",
          "dataAttribute": "data-split"
        },
        "mirror": {
          "source": "meta",
          "classPattern": ".rf-step--{value}",
          "dataAttribute": "data-mirror"
        }
      },
      "childOrder": ["{content}"]
    },

    "Nav": {
      "block": "nav",
      "root": ".rf-nav",
      "dataRune": "nav",
      "childOrder": ["{content}"],
      "notes": "Interactive rune — Svelte component (Nav.svelte) handles rendering"
    },

    "NavGroup": {
      "block": "nav-group",
      "root": ".rf-nav-group",
      "dataRune": "navgroup",
      "childOrder": ["{content}"]
    },

    "NavItem": {
      "block": "nav-item",
      "root": ".rf-nav-item",
      "dataRune": "navitem",
      "childOrder": ["{content}"]
    },

    "Api": {
      "block": "api",
      "root": ".rf-api",
      "dataRune": "api",
      "modifiers": {
        "method": {
          "source": "meta",
          "default": "GET",
          "classPattern": ".rf-api--{value}",
          "dataAttribute": "data-method"
        },
        "path": {
          "source": "meta",
          "classPattern": ".rf-api--{value}",
          "dataAttribute": "data-path"
        },
        "auth": {
          "source": "meta",
          "classPattern": ".rf-api--{value}",
          "dataAttribute": "data-auth"
        }
      },
      "elements": {
        "header": { "tag": "div", "selector": ".rf-api__header", "source": "structure" },
        "method": { "tag": "span", "selector": ".rf-api__method", "source": "structure", "parent": "header" },
        "path": { "tag": "code", "selector": ".rf-api__path", "source": "structure", "parent": "header" },
        "auth": { "tag": "span", "selector": ".rf-api__auth", "source": "structure", "parent": "header", "condition": "auth" },
        "body": { "tag": "div", "selector": ".rf-api__body", "source": "contentWrapper" }
      },
      "childOrder": ["header", "{content:body}"],
      "notes": "Use [data-method='GET'], [data-method='POST'], etc. for HTTP method color variants"
    },

    "Diff": {
      "block": "diff",
      "root": ".rf-diff",
      "dataRune": "diff",
      "modifiers": {
        "mode": {
          "source": "meta",
          "default": "unified",
          "classPattern": ".rf-diff--{value}",
          "dataAttribute": "data-mode"
        }
      },
      "childOrder": ["{content}"]
    },

    "Chart": {
      "block": "chart",
      "root": ".rf-chart",
      "dataRune": "chart",
      "childOrder": ["{content}"],
      "notes": "Interactive rune — Svelte component (Chart.svelte) handles rendering"
    },

    "MusicPlaylist": {
      "block": "music-playlist",
      "root": ".rf-music-playlist",
      "dataRune": "musicplaylist",
      "childOrder": ["{content}"],
      "notes": "Not yet implemented — listed in UNSTYLED_BLOCKS"
    },

    "MusicRecording": {
      "block": "music-recording",
      "root": ".rf-music-recording",
      "dataRune": "musicrecording",
      "childOrder": ["{content}"],
      "notes": "Not yet implemented — listed in UNSTYLED_BLOCKS"
    },

    "Hint": {
      "block": "hint",
      "root": ".rf-hint",
      "dataRune": "hint",
      "modifiers": {
        "hintType": {
          "source": "meta",
          "default": "note",
          "classPattern": ".rf-hint--{value}",
          "dataAttribute": "data-hint-type"
        }
      },
      "contextModifiers": {
        "Hero": { "suffix": "in-hero", "selector": ".rf-hint--in-hero" },
        "Feature": { "suffix": "in-feature", "selector": ".rf-hint--in-feature" }
      },
      "elements": {
        "header": { "tag": "div", "selector": ".rf-hint__header", "source": "structure" },
        "icon": { "tag": "span", "selector": ".rf-hint__icon", "source": "structure", "parent": "header" },
        "title": { "tag": "span", "selector": ".rf-hint__title", "source": "structure", "parent": "header" }
      },
      "childOrder": ["header", "{content}"],
      "notes": "Icon resolved from config.icons.hint[hintType]. Common hintType values: note, warning, caution, check."
    },

    "Hero": {
      "block": "hero",
      "root": ".rf-hero",
      "dataRune": "hero",
      "modifiers": {
        "align": {
          "source": "meta",
          "default": "center",
          "classPattern": ".rf-hero--{value}",
          "dataAttribute": "data-align"
        }
      },
      "contextModifiers": {
        "Feature": { "suffix": "in-feature", "selector": ".rf-hero--in-feature" }
      },
      "childOrder": ["{content}"]
    },

    "CallToAction": {
      "block": "cta",
      "root": ".rf-cta",
      "dataRune": "calltoaction",
      "contextModifiers": {
        "Hero": { "suffix": "in-hero", "selector": ".rf-cta--in-hero" },
        "Pricing": { "suffix": "in-pricing", "selector": ".rf-cta--in-pricing" }
      },
      "childOrder": ["{content}"]
    },

    "Figure": {
      "block": "figure",
      "root": ".rf-figure",
      "dataRune": "figure",
      "modifiers": {
        "size": {
          "source": "meta",
          "default": "default",
          "classPattern": ".rf-figure--{value}",
          "dataAttribute": "data-size"
        },
        "align": {
          "source": "meta",
          "default": "center",
          "classPattern": ".rf-figure--{value}",
          "dataAttribute": "data-align"
        }
      },
      "childOrder": ["{content}"]
    },

    "Sidenote": {
      "block": "sidenote",
      "root": ".rf-sidenote",
      "dataRune": "sidenote",
      "modifiers": {
        "style": {
          "source": "meta",
          "default": "sidenote",
          "classPattern": ".rf-sidenote--{value}",
          "dataAttribute": "data-style"
        }
      },
      "childOrder": ["{content}"]
    },

    "Compare": {
      "block": "compare",
      "root": ".rf-compare",
      "dataRune": "compare",
      "modifiers": {
        "layout": {
          "source": "meta",
          "default": "side-by-side",
          "classPattern": ".rf-compare--{value}",
          "dataAttribute": "data-layout"
        }
      },
      "childOrder": ["{content}"]
    },

    "Conversation": {
      "block": "conversation",
      "root": ".rf-conversation",
      "dataRune": "conversation",
      "childOrder": ["{content}"]
    },

    "ConversationMessage": {
      "block": "conversation-message",
      "root": ".rf-conversation-message",
      "dataRune": "conversationmessage",
      "modifiers": {
        "alignment": {
          "source": "meta",
          "default": "left",
          "classPattern": ".rf-conversation-message--{value}",
          "dataAttribute": "data-alignment"
        }
      },
      "childOrder": ["{content}"]
    },

    "Annotate": {
      "block": "annotate",
      "root": ".rf-annotate",
      "dataRune": "annotate",
      "modifiers": {
        "style": {
          "source": "meta",
          "default": "margin",
          "classPattern": ".rf-annotate--{value}",
          "dataAttribute": "data-style"
        }
      },
      "childOrder": ["{content}"]
    },

    "AnnotateNote": {
      "block": "annotate-note",
      "root": ".rf-annotate-note",
      "dataRune": "annotatenote",
      "childOrder": ["{content}"]
    },

    "Storyboard": {
      "block": "storyboard",
      "root": ".rf-storyboard",
      "dataRune": "storyboard",
      "modifiers": {
        "style": {
          "source": "meta",
          "default": "clean",
          "classPattern": ".rf-storyboard--{value}",
          "dataAttribute": "data-style"
        },
        "columns": {
          "source": "meta",
          "default": "3",
          "classPattern": ".rf-storyboard--{value}",
          "dataAttribute": "data-columns"
        }
      },
      "inlineStyles": {
        "columns": "--sb-columns"
      },
      "childOrder": ["{content}"],
      "notes": "Inline style sets --sb-columns CSS custom property from columns modifier value"
    },

    "StoryboardPanel": {
      "block": "storyboard-panel",
      "root": ".rf-storyboard-panel",
      "dataRune": "storyboardpanel",
      "childOrder": ["{content}"]
    },

    "Bento": {
      "block": "bento",
      "root": ".rf-bento",
      "dataRune": "bento",
      "modifiers": {
        "columns": {
          "source": "meta",
          "default": "4",
          "classPattern": ".rf-bento--{value}",
          "dataAttribute": "data-columns"
        },
        "gap": {
          "source": "meta",
          "default": "1rem",
          "classPattern": ".rf-bento--{value}",
          "dataAttribute": "data-gap"
        }
      },
      "inlineStyles": {
        "columns": "--bento-columns",
        "gap": "--bento-gap"
      },
      "childOrder": ["{content}"],
      "notes": "Inline styles set --bento-columns and --bento-gap CSS custom properties"
    },

    "BentoCell": {
      "block": "bento-cell",
      "root": ".rf-bento-cell",
      "dataRune": "bentocell",
      "modifiers": {
        "size": {
          "source": "meta",
          "default": "medium",
          "classPattern": ".rf-bento-cell--{value}",
          "dataAttribute": "data-size"
        }
      },
      "elements": {
        "title": { "tag": "name", "selector": ".rf-bento-cell__title", "source": "autoLabel" }
      },
      "childOrder": ["{content}"],
      "notes": "autoLabel maps <name> child tags to data-name='title'"
    },

    "Comparison": {
      "block": "comparison",
      "root": ".rf-comparison",
      "dataRune": "comparison",
      "childOrder": ["{content}"],
      "notes": "Interactive rune — Svelte component (Comparison.svelte) handles rendering"
    },

    "ComparisonColumn": {
      "block": "comparison-column",
      "root": ".rf-comparison-column",
      "dataRune": "comparisoncolumn",
      "childOrder": ["{content}"]
    },

    "ComparisonRow": {
      "block": "comparison-row",
      "root": ".rf-comparison-row",
      "dataRune": "comparisonrow",
      "modifiers": {
        "rowType": {
          "source": "meta",
          "default": "text",
          "classPattern": ".rf-comparison-row--{value}",
          "dataAttribute": "data-row-type"
        }
      },
      "childOrder": ["{content}"]
    },

    "HowTo": {
      "block": "howto",
      "root": ".rf-howto",
      "dataRune": "howto",
      "modifiers": {
        "estimatedTime": {
          "source": "meta",
          "classPattern": ".rf-howto--{value}",
          "dataAttribute": "data-estimated-time"
        },
        "difficulty": {
          "source": "meta",
          "default": "medium",
          "classPattern": ".rf-howto--{value}",
          "dataAttribute": "data-difficulty"
        }
      },
      "elements": {
        "meta": { "tag": "div", "selector": ".rf-howto__meta", "source": "structure", "conditionAny": ["estimatedTime", "difficulty"] },
        "meta-item": { "tag": "span", "selector": ".rf-howto__meta-item", "source": "structure", "parent": "meta" },
        "content": { "tag": "div", "selector": ".rf-howto__content", "source": "contentWrapper" }
      },
      "childOrder": ["meta", "{content:content}"],
      "notes": "meta-item elements show estimatedTime (duration-transformed) and difficulty"
    },

    "TabGroup": {
      "block": "tabs",
      "root": ".rf-tabs",
      "dataRune": "tabgroup",
      "childOrder": ["{content}"],
      "notes": "Interactive rune — uses @refrakt-md/behaviors for progressive enhancement"
    },

    "Tab": {
      "block": "tab",
      "root": ".rf-tab",
      "dataRune": "tab",
      "childOrder": ["{content}"],
      "notes": "Interactive-only, styled by behaviors JS — listed in UNSTYLED_BLOCKS"
    },

    "DataTable": {
      "block": "datatable",
      "root": ".rf-datatable",
      "dataRune": "datatable",
      "modifiers": {
        "searchable": {
          "source": "meta",
          "default": "false",
          "classPattern": ".rf-datatable--{value}",
          "dataAttribute": "data-searchable"
        },
        "sortable": {
          "source": "meta",
          "classPattern": ".rf-datatable--{value}",
          "dataAttribute": "data-sortable"
        },
        "pageSize": {
          "source": "meta",
          "default": "0",
          "classPattern": ".rf-datatable--{value}",
          "dataAttribute": "data-page-size"
        },
        "defaultSort": {
          "source": "meta",
          "classPattern": ".rf-datatable--{value}",
          "dataAttribute": "data-default-sort"
        }
      },
      "childOrder": ["{content}"],
      "notes": "Interactive rune — uses @refrakt-md/behaviors for sort, search, and pagination"
    },

    "Form": {
      "block": "form",
      "root": ".rf-form",
      "dataRune": "form",
      "modifiers": {
        "style": {
          "source": "meta",
          "default": "stacked",
          "classPattern": ".rf-form--{value}",
          "dataAttribute": "data-style"
        },
        "action": {
          "source": "meta",
          "classPattern": ".rf-form--{value}",
          "dataAttribute": "data-action"
        },
        "method": {
          "source": "meta",
          "default": "POST",
          "classPattern": ".rf-form--{value}",
          "dataAttribute": "data-method"
        },
        "success": {
          "source": "meta",
          "classPattern": ".rf-form--{value}",
          "dataAttribute": "data-success"
        },
        "error": {
          "source": "meta",
          "classPattern": ".rf-form--{value}",
          "dataAttribute": "data-error"
        },
        "honeypot": {
