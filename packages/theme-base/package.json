  "peerDependencies": {
    "svelte": "^5.0.0",
    "@sveltejs/kit": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    },
    "@sveltejs/kit": {
      "optional": true
    }
  }
}
