{
  "name": "@refrakt-md/theme-base",
  "description": "Base theme for refrakt.md â€” universal rune config, interactive components, and structural CSS",
  "version": "0.4.0",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/refrakt-md/refrakt.git",
    "directory": "packages/theme-base"
  },
  "bugs": "https://github.com/refrakt-md/refrakt/issues",
  "homepage": "https://github.com/refrakt-md/refrakt",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./svelte": {
      "types": "./svelte/index.ts",
      "svelte": "./svelte/index.ts",
      "default": "./svelte/index.ts"
    },
    "./svelte/registry": {
      "types": "./svelte/registry.ts",
      "svelte": "./svelte/registry.ts",
      "default": "./svelte/registry.ts"
    },
    "./svelte/elements": {
      "types": "./svelte/elements.ts",
      "svelte": "./svelte/elements.ts",
      "default": "./svelte/elements.ts"
    },
    "./svelte/behaviors": {
      "types": "./svelte/behaviors.ts",
      "svelte": "./svelte/behaviors.ts",
      "default": "./svelte/behaviors.ts"
    },
    "./svelte/components/*": "./svelte/components/*"
  },
  "files": [
    "dist",
    "svelte"
  ],
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@refrakt-md/behaviors": "0.4.0",
    "@refrakt-md/transform": "0.4.0",
